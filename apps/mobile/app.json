{
  "expo": {
    "name": "partyapp",
    "slug": "partyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#050505",
    "icon": "./assets/icn.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-m.png",
      "resizeMode": "contain",
      "backgroundColor": "#020202"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.alfe.party.app",
      "icon": "./assets/icn-i.png",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Party App to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Party App to save photos."
      }
    },
    "notification": {
      "icon": "./assets/notif.png"
    },
    "android": {
      "icon": "./assets/icn.png",
      "googleServicesFile": "./google-services.json",
      "package": "com.alfe.party.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/notif.png"
    },
    "scheme": "party-app",
    "extra": {
      "eas": {
        "projectId": "2211064f-52db-449d-ad20-1d791f2a0d50"
      }
    },
    "owner": "alfe-party",
    "originalFullName": "@alfe-party/partyapp",
    "currentFullName": "@alfe-party/partyapp",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Party App to access your photos.",
          "savePhotosPermission": "Allow Party App to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2211064f-52db-449d-ad20-1d791f2a0d50"
    }
  }
}
