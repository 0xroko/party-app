{
  "expo": {
    "name": "partyapp",
    "slug": "partyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#050505",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#050505"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.alfe.party.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#050505"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.alfe.party.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "party-app",
    "extra": {
      "eas": {}
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Party App to access your photos.",
          "savePhotosPermission": "Allow Party App to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
