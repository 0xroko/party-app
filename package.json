    "start:wsl:wifi": "REACT_NATIVE_PACKAGER_HOSTNAME=$(netsh.exe interface ip show address \"Wi-Fi\" | grep 'IP Address' | sed -r 's/.*IP Address:\\W*//') TAMAGUI_TARGET=native expo start --tunnel",
